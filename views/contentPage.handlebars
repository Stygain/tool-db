{{> header}}

<div class="content-block">
	<table class="items-table">
		<tr>
			{{#each header}}
				{{> row_header}}
			{{/each}}
			{{#ifCond header 'length' 0}}
				<th>Delete</th>
			{{/ifCond}}
		</tr>
		{{#each item}}
			{{> row}}
		{{/each}}
	</table>
	{{#each accordionData}}
	    {{> accordion}}
	{{/each}}
</div>

{{> fab}}

{{> modal}}

{{> status}}

{{> footer}}
